import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=in.nextInt();
        }
        int max=Arrays.stream(a).max().getAsInt();
        System.out.print(max);
            
    }
}
