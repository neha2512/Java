import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();  
        int power=sc.nextInt();  
        int sum=0;
        int temp=num;
        while(temp>0){
            int digit=temp%10;  
            sum+=Math.pow(digit,power); 
            temp=temp/10; 
        }
        System.out.println(sum);
    }
}
